{
  "expo": {
    "name": "worke-mobile",
    "slug": "worke-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/w-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/w-gradient-logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/w-gradient-logo.png"
    }
  }
}
